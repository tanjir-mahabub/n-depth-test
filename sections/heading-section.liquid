{% schema %}
{
  "name": "Heading Section",
  "tag": "section",
  "class": "heading-section",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading Text",
      "default": "Welcome to Our Store"
    }
  ],
  "presets": [
    {
      "name": "Heading Section"
    }
  ]
}
{% endschema %}

<div class="section-heading text-center py-5">
  <h2>{{ section.settings.heading }}</h2>
</div>
